{
	"name": "back-end",
	"version": "1.0.0",
	"main": "index.js",
	"engines": {
		"node": "18.x"
	},
	"license": "MIT",
	"type": "commonjs",
	"scripts": {
		"test": "jest --collectCoverage",
		"eslint": "eslint .",
		"dev": "node index.js",
		"develop": "nodemon -r dotenv/config index.js",
		"db:create": "sequelize db:create",
		"db:drop": "sequelize db:drop",
		"db:migrate": "sequelize db:migrate",
		"db:undoAll": "sequelize db:migrate:undo:all",
		"db:seed": "sequelize db:seed:undo:all && sequelize db:seed:all",
		"db:seedUndo": "sequelize db:seed:undo:all",
		"vercel-build": "sequelize db:migrate && sequelize db:seed"
	},
	"dependencies": {
		"@babel/core": "^7.24.5",
		"@babel/node": "^7.23.9",
		"@babel/preset-env": "^7.24.5",
		"@babel/preset-react": "^7.24.1",
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dayjs": "^1.11.2",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"jsonwebtoken": "^9.0.2",
		"morgan": "^1.10.0",
		"pg": "^8.7.3",
		"sequelize": "^6.20.1",
		"sequelize-cli": "^6.4.1",
		"swagger-ui-express": "^4.4.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.2.0",
		"eslint": "^9.2.0",
		"globals": "^15.2.0",
		"jest": "^29.7.0",
		"nodemon": "^3.1.0",
		"supertest": "^7.0.0"
	}
}
